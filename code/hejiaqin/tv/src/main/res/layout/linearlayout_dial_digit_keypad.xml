<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@color/transparent"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="162dp"
            android:layout_height="78dp"
            android:id="@+id/key_one"
            android:background="@drawable/selector_dial_bg"
            android:focusable="true"
            android:gravity="center"
            android:orientation="horizontal">
            <Button
                android:layout_width="150dp"
                android:layout_height="64dp"
                android:textSize="30sp"
                android:gravity="center"
                android:text="1"
                android:textColor="@color/keypad_text_color"
                android:background="@drawable/dial_num_btn_bg"
                android:contentDescription="@null"
                android:soundEffectsEnabled="false" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/key_two"
            android:layout_width="162dp"
            android:layout_height="78dp"
            android:layout_marginLeft="4dp"
            android:focusable="true"
            android:gravity="center"
            android:background="@drawable/selector_dial_bg"
            android:orientation="horizontal">

            <Button
                android:layout_width="150dp"
                android:layout_height="64dp"
                android:textSize="30sp"
                android:gravity="center"
                android:text="2"
                android:textColor="@color/keypad_text_color"
                android:background="@drawable/dial_num_btn_bg"
                android:contentDescription="@null"
                android:soundEffectsEnabled="false" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/key_three"
            android:layout_width="162dp"
            android:layout_height="78dp"
            android:layout_marginLeft="4dp"
            android:focusable="true"
            android:gravity="center"
            android:background="@drawable/selector_dial_bg"
            android:orientation="horizontal">

            <Button
                android:layout_width="150dp"
                android:layout_height="64dp"
                android:textSize="30sp"
                android:gravity="center"
                android:text="3"
                android:textColor="@color/keypad_text_color"
                android:background="@drawable/dial_num_btn_bg"
                android:contentDescription="@null"
                android:soundEffectsEnabled="false" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:orientation="horizontal">

        <LinearLayout
            android:id="@+id/key_four"
            android:layout_width="162dp"
            android:layout_height="78dp"
            android:focusable="true"
            android:gravity="center"
            android:background="@drawable/selector_dial_bg"
            android:orientation="horizontal">

            <Button
                android:layout_width="150dp"
                android:layout_height="64dp"
                android:textSize="30sp"
                android:gravity="center"
                android:text="4"
                android:textColor="@color/keypad_text_color"
                android:background="@drawable/dial_num_btn_bg"
                android:contentDescription="@null"
                android:soundEffectsEnabled="false" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/key_five"
            android:layout_width="162dp"
            android:layout_height="78dp"
            android:layout_marginLeft="4dp"
            android:focusable="true"
            android:gravity="center"
            android:background="@drawable/selector_dial_bg"
            android:orientation="horizontal">

            <Button
                android:layout_width="150dp"
                android:layout_height="64dp"
                android:textSize="30sp"
                android:gravity="center"
                android:text="5"
                android:textColor="@color/keypad_text_color"
                android:background="@drawable/dial_num_btn_bg"
                android:contentDescription="@null"
                android:soundEffectsEnabled="false" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/key_six"
            android:layout_width="162dp"
            android:layout_height="78dp"
            android:layout_marginLeft="4dp"
            android:focusable="true"
            android:gravity="center"
            android:background="@drawable/selector_dial_bg"
            android:orientation="horizontal">

            <Button
                android:layout_width="150dp"
                android:layout_height="64dp"
                android:textSize="30sp"
                android:gravity="center"
                android:text="6"
                android:textColor="@color/keypad_text_color"
                android:background="@drawable/dial_num_btn_bg"
                android:contentDescription="@null"
                android:soundEffectsEnabled="false" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:orientation="horizontal">

        <LinearLayout
            android:id="@+id/key_seven"
            android:layout_width="162dp"
            android:layout_height="78dp"
            android:focusable="true"
            android:gravity="center"
            android:background="@drawable/selector_dial_bg"
            android:orientation="horizontal">

            <Button
                android:layout_width="150dp"
                android:layout_height="64dp"
                android:textSize="30sp"
                android:gravity="center"
                android:text="7"
                android:textColor="@color/keypad_text_color"
                android:background="@drawable/dial_num_btn_bg"
                android:contentDescription="@null"
                android:soundEffectsEnabled="false" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/key_eight"
            android:layout_width="162dp"
            android:layout_height="78dp"
            android:layout_marginLeft="4dp"
            android:focusable="true"
            android:gravity="center"
            android:background="@drawable/selector_dial_bg"
            android:orientation="horizontal">

            <Button
                android:layout_width="150dp"
                android:layout_height="64dp"
                android:textSize="30sp"
                android:gravity="center"
                android:text="8"
                android:textColor="@color/keypad_text_color"
                android:background="@drawable/dial_num_btn_bg"
                android:contentDescription="@null"
                android:soundEffectsEnabled="false" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/key_nine"
            android:layout_width="162dp"
            android:layout_height="78dp"
            android:layout_marginLeft="4dp"
            android:focusable="true"
            android:gravity="center"
            android:background="@drawable/selector_dial_bg"
            android:orientation="horizontal">

            <Button
                android:layout_width="150dp"
                android:layout_height="64dp"
                android:textSize="30sp"
                android:gravity="center"
                android:text="9"
                android:textColor="@color/keypad_text_color"
                android:background="@drawable/dial_num_btn_bg"
                android:contentDescription="@null"
                android:soundEffectsEnabled="false" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:orientation="horizontal">

        <LinearLayout
            android:id="@+id/key_asterisk"
            android:layout_width="162dp"
            android:layout_height="78dp"
            android:focusable="true"
            android:gravity="center"
            android:background="@drawable/selector_dial_bg"
            android:orientation="horizontal">

            <Button
                android:layout_width="150dp"
                android:layout_height="64dp"
                android:textSize="30sp"
                android:gravity="center"
                android:text="*"
                android:textColor="@color/keypad_text_color"
                android:background="@drawable/dial_num_btn_bg"
                android:contentDescription="@null"
                android:soundEffectsEnabled="false" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/key_zero"
            android:layout_width="162dp"
            android:layout_height="78dp"
            android:layout_marginLeft="4dp"
            android:focusable="true"
            android:gravity="center"
            android:background="@drawable/selector_dial_bg"
            android:orientation="horizontal">

            <Button
                android:layout_width="150dp"
                android:layout_height="64dp"
                android:textSize="30sp"
                android:gravity="center"
